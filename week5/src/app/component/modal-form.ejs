              <label class="form-check-label" for="editTask"> Task menu </label>
            </div>
          </div>
          <div class="form-group">
            <h6 class="bold text-l">Deactivate Account</h6>
          <div class="d-flex align-items-center danger c-mb-2">
            <i class="material-icons">warning</i> <span class="text-m">This cannot be undone!</span>
          </div>
            <button type="button" class="btn btn-primary btn-lg btn-block danger bg-none btn-outline-danger border-danger">Deactivate This Account</button>
          </div>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
        <button type="button" class="btn btn-primary">Update</button>
      </div>
    </div>
  </div>
</div>
